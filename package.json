{
  "name": "shotfelice",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "create-subset": "npm-run-all -s font:subset font:woff-normal font:woff-bold",
    "font:subset": "node ./lib/subset-japanese.js",
    "font:woff-normal": "pyftsubset ./public/fonts/TsukushiAMaruGothic.ttc --flavor=woff --font-number=0 --layout-features='' --text-file=./public/fonts/subset.txt --output-file=./public/fonts/TsukushiAMaruGothic.woff",
    "font:woff-bold": "pyftsubset ./public/fonts/TsukushiAMaruGothic.ttc --flavor=woff --font-number=1 --layout-features='' --text-file=./public/fonts/subset.txt --output-file=./public/fonts/TsukushiAMaruGothic-Bold.woff",
    "font:woff-normal": "pyftsubset ./public/fonts/TsukushiAMaruGothic.ttc --flavor=woff2 --font-number=0 --layout-features='' --text-file=./public/fonts/subset.txt --output-file=./public/fonts/TsukushiAMaruGothic.woff2",
    "font:woff-bold": "pyftsubset ./public/fonts/TsukushiAMaruGothic.ttc --flavor=woff2 --font-number=1 --layout-features='' --text-file=./public/fonts/subset.txt --output-file=./public/fonts/TsukushiAMaruGothic-Bold.woff2"
  },
  "dependencies": {
    "framer-motion": "^6.2.3",
    "gray-matter": "^4.0.3",
    "next": "12.0.10",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.3",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10"
  }
}